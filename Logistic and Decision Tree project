        "tree_rfe.fit(X_train_rfe, Y_train)\n",
        "#prediction solving\n",
        "y_pred_rfe = tree_rfe.predict(X_test_rfe)\n",
        "#evalute the model to see how well it is doing\n",
        "accuracy_rfe = accuracy_score(Y_test, y_pred_rfe)\n",
        "print(\"Accuracy:\", accuracy_rfe)\n",
        "print(\"Classification Report:\\n\", classification_report(Y_test, y_pred_rfe))\n",
        "print('\\nConfusion Matrix\" \\n)', confusion_matrix(Y_test, y_pred_rfe))"
      ],
