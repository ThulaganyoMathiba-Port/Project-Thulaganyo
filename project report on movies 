      "source": [
        "from textblob import TextBlob\n",
        "# Fill any potential NaN values in the 'description' column with an empty string\n",
        "series_movies['description'] = series_movies['description'].fillna('')\n",
        "\n",
        "# Now apply the TextBlob function\n",
        "series_movies['Polarity'] = series_movies['description'].apply(lambda x: TextBlob(x).sentiment.polarity)\n",
        "series_movies['Sentiment Label'] = series_movies['Polarity'].apply(lambda x: 'POSTIVE' if x > 0 else 'NEGATIVE')\n",
        "series_movies.head()"
