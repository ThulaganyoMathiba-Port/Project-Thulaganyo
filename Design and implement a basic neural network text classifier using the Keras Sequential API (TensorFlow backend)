        "!pip install tensorflow\n",
        "import tensorflow as tf\n",
        "from tensorflow.keras.preprocessing.text import Tokenizer\n",
        "from tensorflow.keras.preprocessing.sequence import pad_sequences\n",
        "\n",
        "#tokenize and pad texts data\n",
        "T = Tokenizer(num_words=5000) # oov_token='<OOV>')\n",
        "T.fit_on_texts(article_data['Headline'])\n",
        "word_index = T.word_index\n",
        "seq = T.texts_to_sequences(article_data['Headline'])\n",
        "padded = pad_sequences(seq, maxlen=20, padding='post') # truncating='post')\n",
        "\n",
        "#labels\n",
        "labs= np.asarray(article_data['Label']).astype('float32')"
