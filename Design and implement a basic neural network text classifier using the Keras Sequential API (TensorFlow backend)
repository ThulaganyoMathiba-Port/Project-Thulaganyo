    {
      "cell_type": "code",
      "source": [
        "from sklearn.model_selection import train_test_split\n",
        "x_train, x_test, y_train, y_test = train_test_split(article_data['Headline'], article_data['Label'], test_size=0.2)\n",
        "\n",
        "import re\n",
        "#normalize by removing things we dont wang\n",
        "def normalize(data):\n",
        "  normalized = []\n",
        "  for i in data:\n",
        "    i = i.lower()\n",
        "    #no urls\n",
        "    i = re.sub('https?://\\S+|www\\.\\S+', '', i)\n",
        "    #no html\n",
        "    i = re.sub(' +', '', i)\n",
        "    i = re.sub('^', '', i)\n",
        "    i = re.sub('$', ' ', i)\n",
        "    i= re.sub('\\n', '', i)\n",
        "    i = re.sub('\\\\W' , ' ', i)\n",
        "    #no punctuation\n",
        "    normalized.append(i)\n",
        "  return normalized\n",
        "\n",
        "\n",
        "x_train = normalize(x_train)\n",
        "x_test = normalize(x_test)"
      ],
      "metadata": {
        "id": "zw-oQhonVRYh"
      },
      "execution_count": null,
      "outputs": []
